package com.example;

public class User {
	// variables
	String name;
	int age;
	
	// constructor: name will be same as class with no return type
	User() {
		System.out.println("User() constructor");
	}
	
	// method that displays name and age
	void userDetails() {
		System.out.println("Name = "+name+", Age = "+age);
	}
}
